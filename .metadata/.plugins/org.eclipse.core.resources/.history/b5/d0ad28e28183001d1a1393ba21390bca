#include "uart_communication_fsm.h"

int uart_communication_status = 0;

uint32_t ADC_value = 0;

void uart_communication_fsm()
{
	ADC_value = HAL_ADC_GetValue (& hadc1 ) ;
		  HAL_UART_Transmit (& huart2 , ( void *) str , sprintf ( str , "%d\n"
		  , ADC_value ) , 1000) ;
}

